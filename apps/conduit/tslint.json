{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "realworldPagePattern",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      ["realworld-page-pattern", "rpp"],
      "kebab-case"
    ]
  },
  "linterOptions": {
    "exclude": ["!**/*"]
  }
}
